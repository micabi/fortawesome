{
  "name": "nodetest",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build": "npx simplifyify ts/index.ts -o js/bundle.js --bundle --debug --minify",
    "watch": "npx simplifyify ts/index.ts -o js/bundle.js --bundle --debug --minify --watch",
    "browse": "npx browser-sync start --server --files '**/*'",
    "bs": "browser-sync start --config bs-config.js"
  },
  "browserify": {
    "plugins": [
      [
        "tsify",
        {
          "target": "es5",
          "module": "commonjs",
          "moduleResolution": "node",
          "jsx": "react"
        }
      ]
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "useBuiltIns": "usage",
                "corejs": 3,
                "debug": true
              }
            ]
          ]
        }
      ],
      [
        "uglifyify",
        {
          "mangle": true,
          "compress": {
            "sequences": true,
            "dead_code": true,
            "booleans": true,
            "conditionals": true,
            "if_return": false,
            "drop_console": false,
            "keep_fnames": true
          },
          "output": {
            "comments": false
          }
        }
      ]
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browsers": [
    "last 2 versions",
    "> 5%",
    "ie >= 10",
    "Android >= 4",
    "ios_saf >= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@jsdevtools/simplifyify": "^8.0.1",
    "@types/bootstrap": "^4.3.1",
    "@types/jquery": "^3.3.33",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babelify": "^10.0.0",
    "browser-sync": "^2.29.1",
    "compass-importer": "^0.4.1",
    "core-js": "^3.6.4",
    "css-declaration-sorter": "^5.1.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-csscomb": "^3.1.0",
    "gulp-header": "^2.0.9",
    "gulp-imagemin": "^7.1.0",
    "gulp-mode": "^1.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "jsdom-quokka-plugin": "^1.0.15",
    "node-sass": "^4.13.1",
    "postcss-assets": "^5.0.0",
    "prettier": "^2.0.1",
    "quokka-jquery-loader": "0.0.3",
    "sprite-magic-importer": "^1.6.2",
    "ts-node": "^8.8.1",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1"
  }
}
